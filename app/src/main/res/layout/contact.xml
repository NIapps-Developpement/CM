<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/gris">
        <ImageButton
            android:layout_margin="7dp"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:id="@+id/drawer"
            android:background="@drawable/ic_menu_white_24dp"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/comite"
            android:textColor="@color/gris_clair"
            android:text="ComitÃ© de la\nTable Ronde"
            android:textSize="60dp"
            android:layout_marginTop="30dp"
            android:textAlignment="center"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="@string/infoc"
        android:id="@+id/infoc"
        android:layout_marginLeft="10dp"
        android:scrollbars="vertical"
        android:layout_below="@+id/comite"
        android:textColor="@color/gris_clair"
        android:layout_margin="10dp"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="@string/pres"
        android:id="@+id/contpres"
        android:layout_below="@+id/infoc"
        android:layout_marginLeft="10dp"
        android:scrollbars="vertical"
        android:textColor="@color/gris_clair"
        android:layout_margin="10dp"
        />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="@string/vp"
        android:layout_marginLeft="10dp"
        android:id="@+id/contvpres"
        android:scrollbars="vertical"
        android:layout_below="@+id/contpres"
        android:textColor="@color/gris_clair"
        android:layout_margin="10dp"
        />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="@string/vpe"
        android:layout_marginLeft="10dp"
        android:scrollbars="vertical"
        android:id="@+id/contvprese"
        android:layout_below="@+id/contvpres"
        android:textColor="@color/gris_clair"
        android:layout_margin="10dp"
